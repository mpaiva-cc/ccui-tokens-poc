  "opacity": {
    "0": {
      "value": 0,
      "description": "Fully transparent (0%). Use for: hidden elements, animation start states, invisible interactive areas. Element is invisible but may still be interactive. CSS: opacity: 0; visibility: hidden for truly hidden.",
      "type": "number"
    },
    "5": {
      "value": 0.05,
      "description": "5% opacity - barely visible. Use for: extremely subtle hover states, ghost backgrounds, near-invisible separators. Only visible against high-contrast backgrounds.",
      "type": "number"
    },
    "10": {
      "value": 0.1,
      "description": "10% opacity - subtle transparency. Use for: light hover backgrounds, subtle shadows, faint overlays. Minimally perceptible change.",
      "type": "number"
    },
    "20": {
      "value": 0.2,
      "description": "20% opacity - light transparency. Use for: hover states, light overlays, muted backgrounds. Noticeable but unobtrusive.",
      "type": "number"
    },
    "25": {
      "value": 0.25,
      "description": "25% opacity - quarter visible. Use for: skeleton loading states, light dimming, subtle backgrounds. Common for placeholder content.",
      "type": "number"
    },
    "30": {
      "value": 0.3,
      "description": "30% opacity - moderate transparency. Use for: image overlays, text shadows, moderate dimming. Visible but allows content behind to show clearly.",
      "type": "number"
    },
    "40": {
      "value": 0.4,
      "description": "40% opacity - semi-transparent. Use for: medium overlays, watermarks, partially hidden content. Balanced visibility.",
      "type": "number"
    },
    "50": {
      "value": 0.5,
      "description": "50% opacity - half visible. Use for: disabled elements (semantic alias: 'disabled'), balanced overlays, de-emphasized content. CAUTION: May not meet contrast requirements for text.",
      "type": "number"
    },
    "60": {
      "value": 0.6,
      "description": "60% opacity - mostly visible. Use for: modal backdrops (semantic alias: 'overlay'), dimmed backgrounds, focus-drawing overlays. Content behind is visible but de-emphasized.",
      "type": "number"
    },
    "70": {
      "value": 0.7,
      "description": "70% opacity - high visibility. Use for: heavy overlays, strong dimming, de-emphasized backgrounds. Most content visible but clearly secondary.",
      "type": "number"
    },
    "75": {
      "value": 0.75,
      "description": "75% opacity - three-quarters visible. Use for: dark overlays, strong backgrounds, significant dimming. Mostly solid with hint of transparency.",
      "type": "number"
    },
    "80": {
      "value": 0.8,
      "description": "80% opacity - mostly solid. Use for: nearly-opaque overlays, frosted glass effects (with backdrop-filter), heavy backgrounds.",
      "type": "number"
    },
    "90": {
      "value": 0.9,
      "description": "90% opacity - nearly solid. Use for: subtle transparency effects, slight see-through, glass-like surfaces. Almost indistinguishable from solid.",
      "type": "number"
    },
    "100": {
      "value": 1,
      "description": "Fully opaque (100%). Use for: solid elements, removing inherited transparency, default element state. CSS: opacity: 1; (default browser value).",
      "type": "number"
    },
    "disabled": {
      "value": 0.5,
      "description": "Disabled element opacity (50%). SEMANTIC alias for opacity.50. Use for: disabled buttons, inactive controls, unavailable options. Universally recognized as 'inactive'. ACCESSIBILITY: Ensure disabled elements are still perceivable and their disabled state is communicated via aria-disabled.",
      "type": "number"
    },
    "overlay": {
      "value": 0.6,
      "description": "Modal overlay opacity (60%). SEMANTIC alias for opacity.60. Use for: modal backdrops, lightbox backgrounds, focus-drawing overlays. Dims background while keeping it visible for context. Common in: Modal backdrop, Drawer overlay, image lightbox backgrounds.",
      "type": "number"
    }
  },
